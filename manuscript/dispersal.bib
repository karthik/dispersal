isbn = {0012-9658},
journal = {Ecology},
keywords = {bovine tuberculosis,chronic disease,colonizing populations,dama mesopotamica,epidemiology,infectious disease,invading organisms,invasion,invasive species,israel,managed disease outbreak,mycobacterium-bovis,persian fallow deer,population expansion,reintroduction,spatially explicit model,spread,transmission,white-tailed deer,wildlife},

author = {Ebert, D and Hottinger, J W and Pajunen, V I},
isbn = {0012-9658},
journal = {Ecology},
keywords = {bacteria,cladocera,clones,colonization,competition, interspecific,daphnia magna,endoparasite,epibiont,epibiont communities,interspecific competition,magna crustacea,metapopulation,microparasites,microsporidian parasite,parasite richness,population,protozoa,pulex,rock pools,spatial patterns,temporal patterns},
number = {12},

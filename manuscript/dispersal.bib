isbn = {0012-9658},
journal = {Ecology},
keywords = {bottom-up,community regulation,consumer-resource dynamics,exploitation ecosystems,herbivore,indirect effect,meta-analysis,metaanalysis,natural communities,population control,predation,predator,productivity,terrestrial,trophic cascade},
number = {2},
pages = {528--537},
title = {{What determines the strength of a trophic cascade?}},
